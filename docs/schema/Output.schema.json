{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://deltares.github.io/Ribasim/schema/Output.schema.json",
    "title": "Output",
    "description": "A Output object based on Ribasim.config.Output",
    "type": "object",
    "properties": {
        "basin": {
            "format": "default",
            "type": "string",
            "default": "output/basin.arrow"
        },
        "flow": {
            "format": "default",
            "type": "string",
            "default": "output/flow.arrow"
        },
        "control": {
            "format": "default",
            "type": "string",
            "default": "output/control.arrow"
        },
        "outstate": {
            "format": "default",
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ],
            "default": null
        },
        "compression": {
            "format": "default",
            "type": "string",
            "default": "zstd"
        },
        "compression_level": {
            "format": "default",
            "type": "integer",
            "default": 6
        }
    },
    "required": [
        "basin",
        "flow",
        "control",
        "compression",
        "compression_level"
    ]
}

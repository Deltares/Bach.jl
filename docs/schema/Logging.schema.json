{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://deltares.github.io/Ribasim/schema/Logging.schema.json",
    "title": "Logging",
    "description": "A Logging object based on Ribasim.config.Logging",
    "type": "object",
    "properties": {
        "verbosity": {
            "format": "default",
            "type": "string",
            "default": "info"
        },
        "timing": {
            "format": "default",
            "type": "boolean",
            "default": false
        }
    },
    "required": [
        "verbosity",
        "timing"
    ]
}

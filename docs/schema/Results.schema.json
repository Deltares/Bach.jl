{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://deltares.github.io/Ribasim/schema/Results.schema.json",
    "title": "Results",
    "description": "A Results object based on Ribasim.config.Results",
    "type": "object",
    "properties": {
        "outstate": {
            "format": "default",
            "anyOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ],
            "default": null
        },
        "compression": {
            "format": "default",
            "type": "string",
            "default": "zstd"
        },
        "compression_level": {
            "format": "default",
            "type": "integer",
            "default": 6
        },
        "subgrid": {
            "format": "default",
            "type": "boolean",
            "default": false
        }
    },
    "required": [
        "compression",
        "compression_level",
        "subgrid"
    ]
}

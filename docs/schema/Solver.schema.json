{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "reltol": {
            "format": "double",
            "default": 0.001,
            "description": "reltol",
            "type": "number"
        },
        "saveat": {
            "format": "default",
            "anyOf": [
                {
                    "items": {
                        "type": "number"
                    },
                    "type": "array"
                },
                {
                    "type": "number"
                }
            ],
            "default": [
            ],
            "description": "saveat"
        },
        "maxiters": {
            "format": "default",
            "default": 1000000000,
            "description": "maxiters",
            "type": "integer"
        },
        "autodiff": {
            "format": "default",
            "default": true,
            "description": "autodiff",
            "type": "boolean"
        },
        "adaptive": {
            "format": "default",
            "default": true,
            "description": "adaptive",
            "type": "boolean"

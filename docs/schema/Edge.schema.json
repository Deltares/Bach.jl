{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://deltares.github.io/Ribasim/schema/Edge.schema.json",
    "title": "Edge",
    "description": "A Edge object based on Ribasim.EdgeV1",
    "type": "object",
    "properties": {
        "fid": {
            "format": "default",
            "type": "integer"
        },
        "name": {
            "format": "default",
            "type": "string"
        },
        "from_node_id": {
            "format": "default",
            "type": "integer"
        },
        "to_node_id": {
            "format": "default",
            "type": "integer"
        },
        "edge_type": {
            "format": "default",
            "type": "string"
        },
        "remarks": {
            "description": "a hack for pandera",
            "type": "string",
            "format": "default",
            "default": ""
        }
    },
    "required": [
        "fid",
        "name",
        "from_node_id",
        "to_node_id",
        "edge_type"
    ]
}
